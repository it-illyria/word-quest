{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/src/Services/(.*)",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/Services/mockApi.ts"
    },
    {
      "src": "/(.*)\\.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "status": 200
    }
  ]
}
